Windows Registry Editor Version 5.00

; SecureGuard URL Scheme Registration
; Run this file or use the install script to register the secureguard:// URL scheme

[HKEY_CLASSES_ROOT\secureguard]
@="URL:SecureGuard VPN Protocol"
"URL Protocol"=""

[HKEY_CLASSES_ROOT\secureguard\shell]

[HKEY_CLASSES_ROOT\secureguard\shell\open]

[HKEY_CLASSES_ROOT\secureguard\shell\open\command]
@="\"C:\\Program Files\\SecureGuard\\secureguard_client.exe\" \"%1\""
